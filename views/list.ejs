
    <%- include('header') -%>

<div class="box" id="heading">
<h1><%=listTitle%></h1>
</div>

<div class="box">
  <% newListItem.forEach(c => { %>
 <div class="item">
   <input type="checkbox">
   <p><%= c %></p>
 </div>
  <% })  %>

  <form class="item" action="/" method="post">
  <input type="text" name="newItem" placeholder="New Item..." autocomplete="off">
  <button type="submit" name="button" value="<%=listTitle%>">+</button>
  </form>

</div>


<%- include('footer') -%>
